# Copy to .env and fill in your secrets
# Do NOT commit .env to git â€” it will be ignored automatically

# Groq API Key (required for the pipeline)
# Get it from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# Model configurations (optional; defaults are in rag_config.py)
ANSWER_MODEL_ID=llama-3.3-70b-versatile
JUDGE_MODEL_ID=llama-3.3-70b-versatile

# Embeddings model (optional)
EMBEDDING_MODEL_ID=BAAI/llm-embedder
FINANCE_EMBEDDING_MODEL_ID=BAAI/llm-embedder

# RAGBench dataset config (optional)
RAGBENCH_DATASET_ID=galileo-ai/ragbench
RAGBENCH_SUBSET=finqa
RAGBENCH_SPLIT=test
RAGBENCH_MAX_ROWS=2000
